<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
    <connectionStrings>
    <add name="CarRental" 
         connectionString="Data Source=.;Initial Catalog=CarRental;Integrated Security=True" 
         providerName="System.Data.SqlClient" />
  </connectionStrings>
    
  <system.serviceModel>
    <services>
      <service name="CarRental.Business.Managers.InventoryManager">
        <endpoint address="net.tcp://localhost:1991/InventoryService"
                  binding="netTcpBinding" 
                  contract="CarRental.Business.Contracts.IInventoryService"/>
      </service>
      
    <service name="CarRental.Business.Managers.RentalManager">
      <endpoint address="net.tcp://localhost:1991/RentalService"
                binding="netTcpBinding"
                contract="CarRental.Business.Contracts.IRentalService"/>
    </service>
  
    <service name="CarRental.Business.Managers.AccountManager">
      <endpoint address="net.tcp://localhost:1991/AccountService"
                binding="netTcpBinding"
                contract="CarRental.Business.Contracts.IAccountService"/>
    </service>
    </services>
  
  <bindings>
    <netTcpBinding>
      <binding transactionFlow="true" maxReceivedMessageSize="2000000">
        <reliableSession enabled="true"/>
      </binding>
    </netTcpBinding>
  </bindings>
  </system.serviceModel>
    
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
</configuration>